(* Straight Insertion pg 80/81 *)

let straight_insertion_sort list =
  let rec aux list sorted_list = match list, sorted_list with
    | [] , _ -> sorted_list
    | h1 :: t1 , []  -> aux t1 (h1 :: sorted_list)
    | h1 :: t1 , h2 :: t2 -> if h1 <= h2 then aux t1 (h1 :: sorted_list)
			     else 
			       


  in aux list []
  
